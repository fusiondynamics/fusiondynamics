set terminal postscript eps size 5.0,5.0 enhanced color \
font 'Helvetica,18' linewidth 2
set output 'results/05beffP_bis.eps'
set grid
set key on outside
set title "Fusion probability vs time for K = 0 and T = B/5"
set xlabel "{/Symbol t} = {/Symbol w}t"
set ylabel "P({/Symbol t})"
plot [0:10][0:*]"./data/05beff_bis.data" u 1:3 w l title "1000 seeds", \
"./data/05beff_1_5.data" u 1:3 w l title "10000 seeds"
